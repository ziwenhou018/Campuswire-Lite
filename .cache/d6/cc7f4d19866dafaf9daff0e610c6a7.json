:71,"column":162},"source":"src/components/Home.js","original":{"line":64,"column":6}},{"generated":{"line":71,"column":163},"source":"src/components/Home.js","original":{"line":41,"column":4}},{"generated":{"line":71,"column":164},"source":"src/components/Home.js","original":{"line":40,"column":2}},{"generated":{"line":72,"column":0},"source":"src/components/Home.js","original":{"line":67,"column":1}},{"generated":{"line":72,"column":1},"source":"src/components/Home.js","original":{"line":5,"column":0}},{"name":"Home","generated":{"line":74,"column":15},"source":"src/components/Home.js","original":{"line":69,"column":15}}],"sources":{"src/components/Home.js":"import axios from 'axios'\nimport React, { useEffect, useState } from 'react'\nimport { Link, useNavigate } from 'react-router-dom'\n\nconst Home = () => {\n  const [username, setUsername] = useState(null)\n  const [questions, setQuestions] = useState([])\n  const [currQuestion, setCurrQuestion] = useState({ _id: '' })\n\n  const navigation = useNavigate()\n\n  const logout = async () => {\n    axios.post('/account/logout').then(() => {\n      navigation('/login')\n    })\n  }\n\n  const checkLoggedIn = async () => {\n    const { data } = await axios.get('/account/isLoggedIn')\n    setUsername(data)\n  }\n\n  const fetchQuestions = async () => {\n    const { data } = await axios.get('/api/questions/')\n    setQuestions(data)\n  }\n\n  const onQuestionClick = question => {\n    if (question._id === currQuestion._id) {\n      setCurrQuestion({ _id: '' })\n    } else {\n      setCurrQuestion(question)\n    }\n  }\n\n  useEffect(() => checkLoggedIn(), [])\n\n  useEffect(() => fetchQuestions(), [])\n\n  return (\n    <div>\n      <div>\n        {username ? (\n          <div>\n            <div>{username}</div>\n            <input type=\"button\" value=\"Log out\" onClick={logout} />\n          </div>\n        ) : (\n          <Link to=\"/login\">Log in</Link>\n        )}\n      </div>\n      <div>\n        {questions.map(question => (\n          <div>\n            <input\n              key={question._id}\n              type=\"button\"\n              value={question.questionText}\n              onClick={() => onQuestionClick(question)}\n            />\n          </div>\n        ))}\n      </div>\n      <div>{currQuestion._id ? <div>{currQuestion._id}</div> : null}</div>\n    </div>\n  )\n}\n\nexport default Home\n"},"lineCount":null}},"error":null,"hash":"40e8f81171376a8d22d1e08d76b31d92","cacheData":{"env":{}}}